<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="XForms.Views.XPlugins.Communicator">
    <ContentPage.Content>
        <StackLayout>
            <Button x:Name="SMS" Text="Communicate in SMS"  Clicked="SMS_Clicked"/>
            <Button x:Name="PhoneCall" Text="Communicate in PhoneCall" Clicked="PhoneCall_Clicked"/>
            <Button x:Name="Email" Text="Communicate in Email" Clicked="Email_Clicked"/>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>
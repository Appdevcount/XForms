<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="XForms.Views.LibraryPOCs.BarcodeScanner">
    <ContentPage.Content>
        <StackLayout>
            <Label Text="Call Barcode Scanner Page from here!"
                VerticalOptions="Start" 
                HorizontalOptions="Center" />
            <Button Text="Scan Barcode" Clicked="ScanBrcde_Clicked" x:Name="ScanBrcde" BackgroundColor="Black" TextColor="White"></Button>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>